@model CreateGuideCommand
@{
    ViewData["Title"] = "AddGuide";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Rehber Ekleme Sayfası MediatR</h1>

<form method="post">
    <label>Rehber Adı</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Rehber Açıklaması</label>
    <input type="text" asp-for="Description" class="form-control"/>
    <br />
    <button class="btn btn-info">Rehberi Kaydet</button>
</form>